var xhr = new XMLHttpRequest();
xhr.open("GET", "https://sites.google.com/view/themattgrossoshowblog/");
xhr.onload = function() {
  if (xhr.status === 200) {
    // The request was successful.
    var blogContent = xhr.responseText;
    // Do something with the blog content.
  } else {
    // The request failed.
  }
};
xhr.send();
